<h1>New User!</h1>
<h3><%= flash[:message] %></h3>

<%= form_with model: @user do |f| %>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <br>
    <%= f.label :user_email %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :user_pass %>
    <%= f.text_field :user_pass %>
    <br>
    <%= f.submit %>
<% end %>